<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.soaic.toolsapp">
    <application
        android:name="com.soaic.toolsapp.SApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:networkSecurityConfig="@xml/network_security_config"
        android:fullBackupContent="false"
        tools:targetApi="p"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".ui.activity.MainActivity"
            android:launchMode="singleTask"
            android:configChanges="orientation|screenSize|keyboardHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.check.CheckActivity" />
        <activity android:name=".ui.activity.music.MusicDetailActivity" />
        <activity android:name=".ui.activity.born.BornActivity" />
        <activity android:name=".ui.activity.location.LocationActivity" />
        <activity android:name=".tinker.app.TinkerActivity"/>
        <activity android:name=".ui.activity.base.BasicWebActivity" />

        <service
            android:name="com.soaic.toolsapp.tinker.service.SampleResultService"
            android:exported="false"/>
        <service android:name=".services.MusicServices" />
    </application>

</manifest>